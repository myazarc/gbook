{
  "name": "book",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test&& jest",
    "test:coverage": "NODE_ENV=test&& jest --coverage",
    "tsc": "tsc",
    "start": "tsc && node ./dist/index.js",
    "start:local": "NODE_ENV=local && ts-node-dev --respawn --transpile-only ./src/index.ts",
    "start:development": "NODE_ENV=development && ts-node-dev --respawn --transpile-only  ./src/index.ts",
    "start:production": "NODE_ENV=production && ts-node-dev --respawn --transpile-only  ./src/index.ts"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "src/test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "json-summary",
      "html"
    ]
  },
  "dependencies": {
    "@types/md5": "^2.3.2",
    "@types/urlencode": "^1.1.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mysql2": "^2.3.3",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.3",
    "sequelize-typescript": "^2.1.3",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.5",
    "jest": "^28.1.3",
    "superagent": "^8.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
